<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>jogosdaqui :: API</title>
	@Styles.Render("~/Content/swagger-ui.css")
	@Styles.Render("~/Content/swagger-ui-complement.css")
	@Styles.Render("~/Content/site.css")
	@Scripts.Render("~/Scripts/jquery-1.8.0.min.js")
	@Scripts.Render("~/Scripts/jquery.slideto.min.js")
	@Scripts.Render("~/Scripts/jquery.wiggle.min.js")
	@Scripts.Render("~/Scripts/jquery.ba-bbq.min.js")
	@Scripts.Render("~/Scripts/jquery.cookie.js")
	@Scripts.Render("~/Scripts/jquery.sha1.js")
</head>
<body>
	<div id="header-top">
		<div class="content-wrap">
			<a href="@ViewData["AbsolutePath"]/docs/"><img src="../Content/jogosdaqui_logo.png" style="width:64px;margin:0;padding-right:120px"  /></a>
			<div class="title">@ViewBag.Title</div>
			<div class="subtitle">@AppsInfo.HttpHandlers.AppVersionService.GetVersion().Number</div>			
		</div>						
	</div>
	<div id="content-body">
		<div class="content-wrap">
			<br/><br/>
			@RenderBody()
		</div>
	</div>
	<div class="clearfix">&nbsp;</div>
	<p style="text-align:center;font-size:8pt;">© Copyright 2013, jogosdaqui.</p>
</body>
</html>
